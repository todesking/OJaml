package test.ml0

module Bool_shorthand {
  let x0 = true && false ;
  (* x0: Bool = false *)

  let x1 = true && true ;
  (* x1: Bool = true *)

  let x2 = true && false && true ;
  (* x2: Bool = false *)

  let y0 = false || false ;
  (* y0: Bool = false *)

  let y1 = false || true ;
  (* y1: Bool = true *)

  let y2 = false || false || true ;
  (* y2: Bool = true *)
}
