package test.ml0

module Jcall {
  (* static call *)
  let x1 = java.lang.Integer##valueOf("123") ;;
  (* x1: java.lang.Integer = 123 *)

  (* instance call *)
  let x2 = 1#toString() ;;
  (* x2: java.lang.String = 1 *)

  (* auto-unboxing *)
  let x3 = java.lang.Integer##valueOf(java.lang.Integer##valueOf(1)) ;;
  (* x3: java.lang.Integer = 1 *)

  (* primitive types *)
  let x4 = 123#intValue() ;;
  (* x4: int = 123 *)
}
