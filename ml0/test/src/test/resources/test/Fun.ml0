package test.ml0

struct Fun {
  let f = fun x: bool =>
    fun y: int =>
      fun z: int =>
        if x then y else z ;;

  let f_true = f true ;;
  let f_false = f false ;;

  let x1 = f true 123 456 ;;
  (* x1: int = 123 *)

  let x2 = f false 123 456 ;;
  (* x2: int = 456 *)

  let x3 = f_true 123 456 ;;
  (* x3: int = 123 *)

  let x4 = f_false 123 456 ;;
  (* x4: int = 456 *)
}
